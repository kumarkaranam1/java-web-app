pipeline {
  agent { label 'linux' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sonarqube-9.9.0.65466') { 
          sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.8.8:sonar'
        }
      }
    }
  }
}
